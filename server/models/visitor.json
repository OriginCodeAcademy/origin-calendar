{
  "name": "Visitor",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "isAdmin": {
      "type": "boolean",
      "default": true
    },
    "slackId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "aptRequests": {
      "type": "hasMany",
      "model": "AptRequest",
      "foreignKey": "visitorId",
      "options": {
        "nestRemoting": true
      }
    },
    "bookedApts": {
      "type": "hasMany",
      "model": "BookedApt",
      "foreignKey": "visitorId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
