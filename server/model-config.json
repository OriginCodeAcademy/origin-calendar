{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "AccessToken": {
    "dataSource": "mongo",
    "public": false
  },
  "ACL": {
    "dataSource": "mongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo",
    "public": true
  },
  "AptRequest": {
    "dataSource": "mongo",
    "public": true
  },
  "BookedApt": {
    "dataSource": "mongo",
    "public": true
  },
  "Slot": {
    "dataSource": "mongo",
    "public": true
  },
  "Visitor": {
    "dataSource": "mongo",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "getUserRole": true
        }
      }
    }
  },
  "DateTime": {
    "dataSource": "mongo",
    "public": true
  }
}
